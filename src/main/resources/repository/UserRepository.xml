<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.microservice.infra.mapper.repository.UserRepository">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.microservice.domain.User">
        <id column="id" property="id" />
        <result column="age" property="age" />
        <result column="name" property="name" />
        <result column="password" property="password" />
        <result column="description" property="description" />
        <result column="cityid" property="cityid" />
        <result column="created_by" property="createdBy" />
        <result column="creation_date" property="creationDate" />
        <result column="last_updated_by" property="lastUpdatedBy" />
        <result column="last_update_date" property="lastUpdateDate" />
        <result column="enabled_flag" property="enabledFlag" />
    </resultMap>

    <!-- 通用查询结果列 createdby, creationdate, lastupdatedby, lastupdatedate, enabledflag-->
    <sql id="Base_Column_List">
        id, age, name, password, description, cityid, createdBy, creationDate, lastUpdatedBy, lastUpdateDate, enabledFlag
    </sql>

</mapper>
